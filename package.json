{
  "devDependencies": {
    "@parcel/transformer-sass": "^2.12.0",
    "parcel": "^2.12.0",
    "parcel-plugin-custom-dist-structure": "^1.1.20",
    "parcel-resolver-ignore": "^2.2.0"
  },
  "dependencies": {
    "parcel-namer-custom": "^0.2.0"
  },
  "parcel-namer-custom": {
    ".scss$": "styles/[name].[hash].[type]",
    ".js$": "scripts/[name].[hash].[type]"
  },
  "targets": {
    "default": {
      "distDir": "dist"
    }
  }
}
